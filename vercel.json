{
    "version":2,
    "builds":[
        {
            "src":"dist/server.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"dist/server.js"
        },
        
        { "src": "/api/orders/create-order", 
        "dest": "/api/orders" },
        { "src": "/api/products/create-product", 
        "dest": "/api/products" }
          
    ]
}