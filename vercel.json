{
    "version":2,
    "builds":[
        {
            "src":"dist/server.js",
            "use":"@vercel/node"
        }
    ],
    
        "rewrites": [
          { "source": "/api/orders", "destination": "/api/orders" }
        ],
      
    "routes":[
        {
            "src":"/(.*)",
            "dest":"dist/server.js"
        }
        
       
          
    ]
    
}